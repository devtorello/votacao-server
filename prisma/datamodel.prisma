type User {
  id: ID! @id
  firstName: String!
  lastName: String!
  RA: String! @unique
  password: String!
  level: String! @default(value: "USER")
  votes: [Vote] @relation(link: INLINE)
}

type Candidate {
  id: ID! @id
  fullName: String!
  RA: String! @unique
  Turma: String!
  URL: String!
}

type Vote {
  id: ID! @id
  candidateRA: String! 
  userId: String! @unique
}
